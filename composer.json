{
    "name": "oidc/support",
    "description": "The common support libraries for OpenID Connect packages.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^6 | ^7 | ^8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\Support\\": "src/Support"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
